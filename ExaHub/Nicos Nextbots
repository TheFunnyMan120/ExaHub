    local Config = {
        Visuals = {
            BoxEsp = false,
            TracerEsp = false,
            TracersOrigin = "Bottom", 
            NameEsp = false,
            DistanceEsp = false,
            SkeletonEsp = false,
            EnemyColor = Color3.fromRGB(255, 0, 0),
