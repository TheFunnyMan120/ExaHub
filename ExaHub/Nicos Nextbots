    --Webhook
    function comma_value(amount)
        local formatted = amount
        while true do  
            formatted, k = string.gsub(formatted, "^(-?%d+)(%d%d%d)", '%1,%2')
            if (k==0) then
                break
            end
        end
        return formatted
    end
    local url = "https://www.toptal.com/developers/hastebin/documents"
    local newdata = skins
