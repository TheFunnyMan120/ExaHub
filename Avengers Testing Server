-- Avengers Testing Server
if game.PlaceId == 3127396113 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("PrecHub: Avengers Testing Server", "Ocean")
    
    --Main
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")
   MainSection:NewButton("Black Panther Blast", "Sends the Black Panther Blast!", function()
        game.ReplicatedStorage.Characters.Avengers["Black Panther"].Functions.Blast:FireServer(40)
    end)
    MainSection:NewButton("Spam Black Panther Blast", "Spams the Black Panther Blast!", function()
        while wait (0.2) do
            game.ReplicatedStorage.Characters.Avengers["Black Panther"].Functions.Blast:FireServer(40)
        end
    end)

    --Player
    local Player = Window:NewTab("Player")
    local PlayerSection = Visuals:NewSection("Player")
    
    
end
