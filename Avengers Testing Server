-- Avengers Testing Server
if game.PlaceId == 3127396113 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("PrecHub: Avengers Testing Server", "Ocean")
    
    --Main
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")
    --Player
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")
    PlayerSection:NewButton("Black Panther Blast", "Sends the Black Panther Blast!", function()
        game.ReplicatedStorage.Characters.Avengers["Black Panther"].Functions.Blast:FireServer(40)
    end)
    PlayerSection:NewButton("Spam Black Panther Blast", "Spams the Black Panther Blast!", function()
        while wait (0.2) do
            game.ReplicatedStorage.Characters.Avengers["Black Panther"].Functions.Blast:FireServer(40)
        end
    end)
    PlayerSection:NewButton("Call Iron Man Suit", "Calls the Mark 85 Suit!", function()
        game.ReplicatedStorage.Characters.Avengers["Iron Man"].Functions.Suit:InvokeServer("Mark85","CallSuit")
    end)
    PlayerSection:NewButton("Summon Shield", "Calls Captain America's shield!", function()
        game.ReplicatedStorage.Characters.Avengers["Captain America"].Functions.Setup:InvokeServer("equip", "Default")
    end)
    PlayerSection:NewButton("Summon Wings", "Summons Falcon's wings!", function()
        game.ReplicatedStorage.Characters.Avengers.Falcon.Functions.Aesthetic:FireServer("giveSuitPack", "NewExo")
	wait (0.1)
	game.ReplicatedStorage.Characters.Avengers.Falcon.Functions.Aesthetic:FireServer("changeExoWings", 0)
    end)
    PlayerSection:NewButton("Grow", "Grow.", function()
        game.ReplicatedStorage.Characters.Avengers.Hulk.Functions.Grow:FireServer("Grow")
    end)
    
    
    
end
